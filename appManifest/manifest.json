{
  "$schema": "https://developer.microsoft.com/json-schemas/teams/v1.19/MicrosoftTeams.schema.json",
  "manifestVersion": "1.19",
  "version": "1.0.0",
  "id": "<app_id>",
  "developer": {
    "name": "Databricks, inc",
    "websiteUrl": "https://www.databricks.com/",
    "privacyUrl": "https://www.databricks.com/",
    "termsOfUseUrl": "https://www.databricks.com/"
  },
  "name": {
    "short": "Teams-Databricks Auth OBO",
    "full": "Teams-Databricks Auth On Behalf Of The User"
  },
  "description": {
    "short": "Demonstrates authorization in Teams against Databricks agent using OOB.",
    "full": "Demonstrates authorization in Teams against Databricks agent using Authentication On Behalf Of The User."
  },
  "icons": {
    "outline": "outline.png",
    "color": "color.png"
  },
  "accentColor": "#abcdef",
  "bots": [
    {
      "botId": "<app_id>",
      "scopes": [
        "personal",
        "groupChat"
      ],
      "isNotificationOnly": false,
      "supportsCalling": false,
      "supportsVideo": false,
      "supportsFiles": false
    }
  ],
  "permissions": [
    "identity",
    "messageTeamMembers"
  ],
  "validDomains": [
    "<web_app_domain>",
    "token.botframework.com"
  ]
}